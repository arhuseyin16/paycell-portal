<+>import { Component, OnInit } from '@angular/core';\nimport {ActivatedRoute} from '@angular/router';\nimport {NgxSpinnerService} from 'ngx-spinner';\nimport {UserService} from '../../../services/user.service';\nimport {MessageService} from '../../../shared/services/message.service';\n\n@Component({\n  selector: 'app-mail-confirmation',\n  templateUrl: './mail-confirmation.component.html',\n  styleUrls: ['./mail-confirmation.component.scss']\n})\nexport class MailConfirmationComponent implements OnInit {\n  code: string;\n  alertMessage: any;\n  spinnerText: string;\n  enumMailValid = {\n    INVALID_LINK: 'INVALID_LINK',\n    EMAIL_EXPIRED: 'EMAIL_EXPIRED',\n    EMAIL_ALREADY_CONFIRMED: 'EMAIL_ALREADY_CONFIRMED',\n    EMAIL_VALIDATED: 'EMAIL_VALIDATED'\n  }\n\n  constructor(private activatedRoute: ActivatedRoute,\n              private spinner: NgxSpinnerService,\n              private userService: UserService,\n              private messageService: MessageService) {\n    this.activatedRoute.queryParams.subscribe(params => {\n      this.code = params['code'];\n      this.mailConfirmation();\n    });\n  }\n\n  ngOnInit(): void {\n  }\n\n  mailConfirmation(): void {\n    if (this.code) {\n      this.initAlertMessage(false, null);\n      this.showSpinner('mail-confirmation.spinner.loading');\n      this.userService.mailValidation(this.code).subscribe(res => {\n        this.mailValidMessage(res);\n      }, (err) => {\n        if (err.error.localizedMessage) {\n          this.initAlertMessage(false, err.error.localizedMessage);\n        }\n        this.messageService.addHttpError({\n          httpErrorResponse: err,\n          defaultMessage: 'mail-confirmation.message.error'\n        });\n        this.resetSpinner();\n      });\n    } else {\n      this.initAlertMessage(false, 'mail-confirmation.message.notCode');\n    }\n  }\n\n  mailValidMessage(response): void {\n    this.resetSpinner();\n    if (response.body.mailValidationHeader.responseCode === '0') {\n      this.initAlertMessage(true, 'mail-confirmation.message.validated');\n    }\n  }\n\n  showSpinner(text: string): void {\n    this.spinnerText = text;\n    this.spinner.show(undefined,\n      {\n        type: 'ball-triangle-path',\n        size: 'medium',\n        bdColor: 'rgba(0, 0, 0, 0.8)',\n        color: '#fff',\n        fullScreen: true\n      });\n  }\n\n  resetSpinner(): void {\n    this.spinnerText = null;\n    this.spinner.hide();\n  }\n\n  initAlertMessage(active: boolean, message: string): void {\n    this.alertMessage = {active, message};\n  }\n\n}\n
